{
    "name": "Todo list",
    "version": "0.0.1",
    "description": "description",
    "permissions": [
        "storage"
    ],
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "background": {
        "service_worker": "sw.js"
    },
    "action": {
        "default_icon": {
            "16": "images/icon_16.png",
            "48": "images/icon_48.png",
            "128": "images/icon_128.png"
        },
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "images/icon_16.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "manifest_version": 3
}